{
    "name": "guttmann/naut-cli",
    "description": "CLI client for SilverStripe deploynaut",
    "type": "project",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "vlucas/phpdotenv": "^2.4",
        "symfony/framework-bundle": "^3.3",
        "symfony/console": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Guttmann\\NautCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guttmann\\NautCli\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "mockery/mockery": "^0.9.8",
        "phpunit/phpunit": "^5.7"
    }
}
