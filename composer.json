{
    "name": "guttmann/naut-cli",
    "description": "CLI client for SilverStripe deploynaut",
    "type": "project",
    "require": {
        "symfony/console": "^3.2",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/dom-crawler": "^3.2",
        "symfony/css-selector": "^3.2",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Guttmann\\NautCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guttmann\\NautCli\\": "tests/"
        }
    },
    "bin": [
        "bin/naut-cli"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "mockery/mockery": "^0.9.8",
        "phpunit/phpunit": "^5.7"
    }
}
